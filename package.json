{
  "scripts": {
    "dev": "pnpm dev-pre && run-p dev:*",
    "dev-pre": "pnpm build:css && bash scripts/copy-assets.sh",
    "dev-e2e": "export NODE_ENV=test && pnpm dev-pre && PORT=3001 pnpm dev:remix",
    "dev-ui": "vite --host",
    "dev:remix": "remix dev",
    "dev-coverage:remix": "c8 -o coverage/e2e-server -r text -r html --exclude build --exclude packages --exclude-after-remap node_modules/.bin/remix dev",
    "tsc": "tsc -b",
    "dev:tsc": "pnpm tsc --watch --preserveWatchOutput",
    "build:css": "unocss 'app/**/*.tsx' --out-file \"./build/css/${NODE_ENV:-development}/index.css\"",
    "dev:css": "pnpm build:css --watch",
    "lint": "run-s lint:*",
    "lint-check": "run-s lint-check:*",
    "lint:isort": "pnpm lint-check:isort --fix",
    "lint-check:isort": "isort-ts --git --cache",
    "lint:prettier": "prettier -w --cache .",
    "lint-check:prettier": "prettier -c --cache .",
    "test": "NODE_ENV=test vitest",
    "test-coverage": "NODE_ENV=test vitest --run --coverage",
    "test-e2e": "NODE_ENV=test playwright test",
    "test-e2e-coverage": "bash scripts/test-e2e-coverage.sh",
    "dump-schema": "echo \"// auto-generated by 'npm run dump-schema'\nexport default $(npm run -s cli -- db:schema --json) as const\" > app/db/schema.ts",
    "build": "bash scripts/vercel.sh",
    "release": "vercel deploy --prebuilt build/remix/production/deploy",
    "release-production": "vercel deploy --prebuilt build/remix/production/deploy --prod",
    "cli": "node --require esbuild-register app/misc/cli.ts",
    "cli-staging": "eval $(bash scripts/env-shell.sh < .env.staging) && node --require esbuild-register app/misc/cli.ts",
    "cli-production": "eval $(bash scripts/env-shell.sh < .env.production) && node --require esbuild-register app/misc/cli.ts",
    "console": "node --experimental-repl-await -r esbuild-register -r ./app/misc/console.ts",
    "console-staging": "eval $(bash scripts/env-shell.sh < .env.staging) && npm run console",
    "knex": "NODE_OPTIONS='-r esbuild-register' knex --knexfile app/db/knexfile.server.ts",
    "knex-staging": "eval $(bash scripts/env-shell.sh < .env.staging) && pnpm knex",
    "knex-production": "eval $(bash scripts/env-shell.sh < .env.production) && pnpm knex",
    "qrcode": "qrcode-terminal http://$(ip -o -4 address list wlan0 | awk '{print $4}' | cut -d / -f 1):3000"
  },
  "dependencies": {
    "@badrap/bar-of-progress": "^0.2.2",
    "@floating-ui/react": "^0.21.1",
    "@headlessui/react": "^1.7.13",
    "@hiogawa/utils": "^1.4.1",
    "@hiogawa/utils-react": "^1.2.0",
    "@remix-run/node": "^1.4.0",
    "@remix-run/react": "^1.4.0",
    "@remix-run/server-runtime": "^1.4.0",
    "@remix-run/vercel": "^1.4.0",
    "@tanstack/react-virtual": "3.0.0-beta.54",
    "@vercel/node": "^1.15.3",
    "bcryptjs": "^2.4.3",
    "echarts": "^5.3.2",
    "fast-xml-parser": "^4.0.7",
    "jotai": "^2.0.3",
    "knex": "^2.4.2",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^2.4.0",
    "react-query": "^3.34.18",
    "react-remove-scroll": "^2.5.5",
    "react-router-dom": "^6.3.0",
    "superjson": "^1.8.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@hiogawa/isort-ts": "1.0.1",
    "@hiogawa/unocss-preset-antd": "2.2.1-pre.1",
    "@iconify-json/ri": "^1.1.5",
    "@playwright/test": "^1.31.2",
    "@remix-run/dev": "1.4.1",
    "@remix-run/serve": "1.4.1",
    "@tsconfig/strictest": "^1.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.179",
    "@types/node": "^16",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@unocss/cli": "^0.50.6",
    "@unocss/reset": "^0.50.6",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "c8": "^7.11.2",
    "cac": "^6.7.12",
    "esbuild": "^0.14.23",
    "esbuild-register": "^3.3.2",
    "express": "^4.17.3",
    "fs-extra": "^10.1.0",
    "gh-pages": "^3.2.3",
    "happy-dom": "^8.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "qrcode-terminal": "^0.12.0",
    "typescript": "^4.9.5",
    "unocss": "^0.50.6",
    "vite": "^4.2.0",
    "vitest": "^0.29.3"
  },
  "volta": {
    "node": "16.15.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "@remix-run/dev@1.4.1": "patches/@remix-run__dev@1.4.1.patch",
      "@remix-run/serve@1.4.1": "patches/@remix-run__serve@1.4.1.patch",
      "@remix-run/node@1.4.1": "patches/@remix-run__node@1.4.1.patch"
    }
  }
}
