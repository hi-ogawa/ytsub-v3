{
  "scripts": {
    "clean": "rm -rf build",
    "dev": "pnpm dev:prepare && run-p remix:dev tsc:dev tailwind:dev",
    "dev:prepare": "pnpm tailwind && bash scripts/copy-assets.sh",
    "dev:extra": "run-p vite",
    "dev:e2e": "NODE_ENV=test npm run dev:prepare && NODE_ENV=test PORT=3001 npm run remix:dev",
    "remix:dev": "remix dev",
    "remix:dev:coverage": "c8 -o coverage/e2e-server -r text -r html --exclude build --exclude packages --exclude-after-remap node_modules/.bin/remix dev",
    "tsc": "tsc",
    "tsc:dev": "tsc --noEmit --watch --preserveWatchOutput",
    "tailwind": "tailwindcss -o \"./build/tailwind/${NODE_ENV:-development}/index.css\"",
    "tailwind:dev": "npm run tailwind -- --watch",
    "vite": "vite --host",
    "lint": "run-s lint:*",
    "lint-check": "run-s lint-check:*",
    "lint:isort": "pnpm lint-check:isort --fix",
    "lint-check:isort": "isort-ts --git --cache",
    "lint:prettier": "prettier -w --cache .",
    "lint-check:prettier": "prettier -c --cache .",
    "test": "NODE_ENV=test vitest",
    "test:coverage": "NODE_ENV=test vitest --run --coverage",
    "test-e2e": "NODE_ENV=test playwright test",
    "test-e2e:coverage": "bash scripts/test-e2e-coverage.sh",
    "dump-schema": "echo \"// auto-generated by 'npm run dump-schema'\nexport default $(npm run -s cli -- db:schema --json) as const\" > app/db/schema.ts",
    "vercel:build": "bash scripts/vercel.sh",
    "vercel:build:deploy:staging": "npm run vercel:build && vercel deploy --prebuilt build/remix/production/deploy",
    "vercel:build:deploy:production": "npm run vercel:build && vercel deploy --prebuilt build/remix/production/deploy --prod",
    "cli": "node --require esbuild-register app/misc/cli.ts",
    "cli:staging": "eval $(bash scripts/env-shell.sh < .env.staging) && node --require esbuild-register app/misc/cli.ts",
    "cli:production": "eval $(bash scripts/env-shell.sh < .env.production) && node --require esbuild-register app/misc/cli.ts",
    "console": "node --experimental-repl-await -r esbuild-register -r ./app/misc/console.ts",
    "console:staging": "eval $(bash scripts/env-shell.sh < .env.staging) && npm run console",
    "knex": "NODE_OPTIONS='-r esbuild-register' knex --knexfile app/db/knexfile.server.ts",
    "knex:staging": "eval $(bash scripts/env-shell.sh < .env.staging) && pnpm knex",
    "knex:production": "eval $(bash scripts/env-shell.sh < .env.production) && pnpm knex",
    "qrcode": "qrcode-terminal http://$(ip -o -4 address list wlan0 | awk '{print $4}' | cut -d / -f 1):3000"
  },
  "dependencies": {
    "@floating-ui/react-dom-interactions": "^0.0.15",
    "@headlessui/react": "^0.0.0-insiders.ab6310c",
    "@hiogawa/utils": "^1.2.0",
    "@remix-run/node": "^1.4.0",
    "@remix-run/react": "^1.4.0",
    "@remix-run/server-runtime": "^1.4.0",
    "@remix-run/vercel": "^1.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@vercel/node": "^1.15.3",
    "bcryptjs": "^2.4.3",
    "echarts": "^5.3.2",
    "fast-xml-parser": "^4.0.7",
    "knex": "^2.4.2",
    "lodash": "^4.17.21",
    "mysql2": "^2.3.3",
    "qs": "^6.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-query": "^3.34.18",
    "react-router-dom": "^6.3.0",
    "react-transition-group": "^4.4.2",
    "react-use": "^17.3.2",
    "superjson": "^1.8.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@hiogawa/isort-ts": "1.0.0-pre.11",
    "@playwright/test": "^1.19.2",
    "@remix-run/dev": "1.4.1",
    "@remix-run/serve": "1.4.1",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@tsconfig/strictest": "^1.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.179",
    "@types/node": "^16",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-transition-group": "^4.4.4",
    "@vitejs/plugin-react": "^1.2.0",
    "autoprefixer": "^10.4.2",
    "c8": "^7.11.2",
    "cac": "^6.7.12",
    "daisyui": "^2.2.2",
    "esbuild": "^0.14.23",
    "esbuild-register": "^3.3.2",
    "express": "^4.17.3",
    "fs-extra": "^10.1.0",
    "gh-pages": "^3.2.3",
    "happy-dom": "^2.50.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.7",
    "prettier": "^2.8.4",
    "qrcode-terminal": "^0.12.0",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.9.5",
    "vite": "^2.8.6",
    "vitest": "^0.7.12"
  },
  "volta": {
    "node": "16.15.0"
  },
  "pnpm": {
    "patchedDependencies": {
      "@remix-run/dev@1.4.1": "patches/@remix-run__dev@1.4.1.patch",
      "@remix-run/serve@1.4.1": "patches/@remix-run__serve@1.4.1.patch"
    }
  }
}
